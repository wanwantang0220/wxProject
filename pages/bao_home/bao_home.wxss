/* pages/bao_home/bao_home.wxss */

.container {
  width: 100%;
  height: 100%;
  background: #f4f4f4;
}

.view_center {
  width: 100%-40rpx;
  align-content: center;
  align-items: center;
  display: flex;
  flex-direction: column;
}

.view_center_child {
  width: 100%-40rpx;
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
}

.view_center_item {
  width: 250rpx;
  height: 200rpx;
  text-align: center;
  font-size: 24px;
  color: #fff;
  background: #fff;
  margin: 20rpx;
  border-radius: 20rpx;
  padding-top: 120rpx;
}
